<% provide(:title, 'All students') %>
<h1>Students</h1>
<ul class="students">
  <table class = "table">
    <thead>
      <tr>
        <th>id</th>
        <th>Name</th>
        <th>Gender</th>
        <th>Phone #</th>
        <th>Address</th>
        <th>Birth Date</th>
        <th>Departments</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
      </thead>
    <tbody>
      <%= will_paginate @students %>
        <% @students.each do |student| %>
          <%= render student %>
        <% end %>
    </tbody>
  </table>
  <%= will_paginate %>
</ul>