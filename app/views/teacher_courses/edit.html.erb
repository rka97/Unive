<% provide(:title, "Edit " + @teacher.name + " - " + @teachercourse.course.code) %>

<div class="row">
  <h4 class="center">Edit <%= link_to @teachercourse.course.code, @teachercourse.course %> data for <%= link_to @teacher.name, @teacher %></h4>
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@teachercourse) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= f.label :date_taught %>
        <%= f.date_field :date_taught, class: 'form-control' %>
        <%= f.submit "Save Changes", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>