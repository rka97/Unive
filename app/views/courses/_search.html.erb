<%= search_form_for @q do |f| %>
    <%= f.label :id_eq, class: 'col-sm-2 col-form-label' %>
    <div class="col-sm-10">
      <%= f.search_field :id_eq, class: 'form-control', type: 'number' %>
    </div>
    <%= f.label :code_cont, class: 'col-sm-2 col-form-label'  %>
    <div class="col-sm-10">
      <%= f.search_field :code_cont, class: 'form-control' %>
    </div>
    <%= f.label :title_cont, class: 'col-sm-2 col-form-label'  %>
    <div class="col-sm-10">
      <%= f.search_field :title_cont, class: 'form-control' %>
    </div>
    <%= f.label :department_name_cont, class: 'col-sm-2 col-form-label'  %>
    <div class="col-sm-10">
     <%= f.search_field :department_name_cont, class: 'form-control' %>
    </div>
    <% if current_user.admin? %>
    <%= f.label :students_name_cont, class: 'col-sm-2 col-form-label'  %>
    <div class="col-sm-10">
     <%= f.search_field :students_name_cont, class: 'form-control' %>
    </div>
    <%= f.label :teachers_name_cont, class: 'col-sm-2 col-form-label'  %>
    <div class="col-sm-10">
     <%= f.search_field :teachers_name_cont, class: 'form-control' %>
    </div>
    <% end %>
    <%= f.submit class: 'btn btn-primary', type: 'submit' %>
<% end %>