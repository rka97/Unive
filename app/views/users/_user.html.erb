<li>
  <%= gravatar_for user, size: 50 %>
  <%= link_to user.name, user %>
   | <%= user.profile_owner_type %>
  <% if current_user.admin? && user != current_user %>
    | <%= link_to "delete", user_path(id: user.id), :method => :delete, :data => {confirm: "Are you sure?"} %>
  <% end%>
</li>